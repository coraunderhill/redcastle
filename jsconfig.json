{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./app",
    "paths": {
      "@common/*": ["src/renderer/common/*"],
      "@components/*": ["src/renderer/components/Global/*"],
      "@darwin/*": ["src/renderer/components/Darwin/*"],
      "@static/*": ["src/renderer/static/*"],
      "@win32/*": ["src/renderer/components/Win32/*"]
    },
    "target": "es2017"
  },
  "exclude": ["node_modules", "build"]
}
